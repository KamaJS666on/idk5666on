local AutoStompButton = Instance.new("TextButton")
local RandomTPFrame = Instance.new("Frame")
local RandomTPLabel = Instance.new("TextLabel")
local RandomTPButton = Instance.new("TextButton")
ScreenGui.ResetOnSpawn = false 
ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
ScreenGui.Name = "Kama666on"

