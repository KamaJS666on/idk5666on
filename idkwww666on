local autoStompEnabled = false
if game.PlaceId == 2788229376 then
    MainEvent = game:GetService('ReplicatedStorage'):WaitForChild('MainEvent')
end

local Players = game:GetService("Players")
local Client = Players.LocalPlayer
local AntiEffect = 34244088
local SafePosition = Vector3.new(0, 100, 0)

RandomTPButton.Text = "OFF"

AutoStompButton.MouseButton1Click:Connect(function()
    autoStompEnabled = not autoStompEnabled
    AutoStompButton.Text = autoStompEnabled and "ON" or "OFF"

    if autoStompEnabled then
        task.spawn(function()
            while autoStompEnabled do
                for _, Player in ipairs(Players:GetPlayers()) do
                    if Player ~= Client and Player.Character and Player:IsInGroup(AntiEffect) == false then
                        local BodyEffects = Player.Character:FindFirstChild("BodyEffects")
                        if BodyEffects and BodyEffects:FindFirstChild("K.O") and BodyEffects["K.O"].Value then
                            local TargetPosition = Player.Character:FindFirstChild("UpperTorso")
                            if TargetPosition then
                                Client.Character:MoveTo(TargetPosition.Position)
                                task.wait(0.3)
                                MainEvent:FireServer("Stomp")
                                task.wait(0.2)
                                Client.Character:MoveTo(SafePosition)
                            end
                        end
                    end
                end
                task.wait(0.5)
            end
        end)
    end
end)

Title.Text = ".gg/sigmabasados"
local RunService = game:GetService("RunService")
local maxRange = 500
local teleporting = false
local function teleportRandomly()
    local player = game.Players.LocalPlayer
    if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
        local newPosition = Vector3.new(
            math.random(-maxRange, maxRange),
            math.random(50, 150),
            math.random(-maxRange, maxRange)
        )
        player.Character.HumanoidRootPart.CFrame = CFrame.new(newPosition)
    end
end

RandomTPButton.MouseButton1Click:Connect(function()
    teleporting = not teleporting
    RandomTPButton.Text = teleporting and "ON" or "OFF"

    if teleporting then
        task.spawn(function()
            while teleporting do
                teleportRandomly()
                task.wait(0.1)
            end
        end)
    end
end)
task.spawn(function()
    while wait() do
        pcall(function()
            local character = game:GetService("Players").LocalPlayer.Character
            if character and character:FindFirstChild("BodyEffects") and character:FindFirstChild("BodyEffects"):FindFirstChild("K.O").Value == true then
                for i, v in pairs(character:GetChildren()) do
                    if v:IsA("BasePart") and v.Name ~= "HumanoidRootPart" then
                        v:Destroy()
                    end
                end
            end
        end)
    end
end)

game.Players.LocalPlayer.CharacterAdded:Connect(function()
    if not ScreenGui.Parent then
        ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
    end
end)

local function onCharacterDied()
    local player = game.Players.LocalPlayer
    player:LoadCharacter() 
end

if Client.Character then
    Client.Character:WaitForChild("Humanoid").Died:Connect(onCharacterDied)
end

AutoStompButton.Text = "OFF"

Client.CharacterAdded:Connect(function()
    Client.Character:WaitForChild("Humanoid").Died:Connect(onCharacterDied)
end)
